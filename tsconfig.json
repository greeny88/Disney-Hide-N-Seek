{
	"compilerOptions": {
		"target": "es5",
		"module": "es6",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [
			"esnext", 
			"dom"
		],
		"noImplicitAny": false,
		"suppressImplicitAnyIndexErrors": true,
		"outDir": "./dist/"
	}
}
